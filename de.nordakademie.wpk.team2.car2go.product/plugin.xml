<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="de.nordakademie.wpk.team2.car2go.product.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="de.nordakademie.wpk.team2.car2go.product.Perspective"
            fixed="true"
            id="de.nordakademie.wpk.product.perspective"
            name="Perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               label="File">
            <command
                  commandId="org.eclipse.ui.file.exit"
                  label="Exit">
            </command>
            <command
                  commandId="org.eclipse.ui.file.save"
                  label="Speichern"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         id="Car2Go"
         point="org.eclipse.core.runtime.products">
      <product
            application="de.nordakademie.wpk.team2.car2go.product.application"
            name="Car2Go">
         <property
               name="appName"
               value="Car2Go">
         </property>
         <property
               name="windowImages"
               value="platform:/plugin/de.nordakademie.wpk.team2.car2go.ui/resources/icons/car2goIcon16x16.png,platform:/plugin/de.nordakademie.wpk.team2.car2go.ui/resources/icons/car2goIcon32x32.png">
         </property>
         <property
               name="startupProgressRect"
               value="20,300,460,20">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>

</plugin>
